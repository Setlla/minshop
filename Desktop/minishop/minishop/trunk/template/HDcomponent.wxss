.address-list .info {
  padding: 10px 15px;
}

.address-list .info text:nth-child(2) {
  float: right;
}

.address-list .address {
  padding: 0 15px 10px 15px;
  border-bottom: 1px solid #e4daee;
}

.address-list .button {
  padding: 0 15px;
}

.address-list .button view {
  float: left;
  font-size: 14px;
}

.address-list .button view image {
  border-radius: 50%;
  top: 5px;
  margin-right: 5px;
}

.address-list .button button {
  float: right;
  background: #fff;
  font-size: 14px;
  padding: 0;
  margin-left: 15px;
}

.address-list .button:after {
  content: "";
  display: block;
  clear: both;
}

.address-list .button image {
  width: 20px;
  height: 20px;
  position: relative;
  top: 4px;
}

.address-list .border {
  border: 1px solid #888;
}

.addr-select view {
  background: #fff;
  padding: 10px;
  font-size: 14px;
}

.my-info {
  background: #fff;
  padding: 0 15px;
}

.my-info .vip {
  width: 10px;
  position: absolute;
  bottom: 14px;
  right: 15px;
  margin-left: 20px;
}

.my-info>view {
  padding: 15px 0;
  position: relative;
  border-bottom: 1px solid #e4daee;
  margin-right: -15px;
  padding-right: 15px;
}

.my-info>view:last-child {
  border: none;
}

.my-info image {
  width: 45px;
  height: 45px;
  vertical-align: middle;
  border-radius: 50%;
  border: solid 1px #ac00ff;
}

.my-info .middle {
  margin-top: 15px;
}

.my-info .left {
  float: left;
}

.my-info .right {
  float: right;
  text-align: right;
}

.discount-select {
  position: fixed;
  z-index: 10;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.discount-select .bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 5;
}

.discount-select .coupon-list {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 6;
  background: #f6f2f8;
}

.discount-select .coupon {
  background: #fff;
  margin: 15px;
}

.discount-select .coupon:after {
  content: "";
  display: block;
  clear: both;
}

.discount-select .coupon>view:nth-child(1) {
  background: #ac00ff;
  color: #fff;
  text-align: center;
  width: 70px;
  height: 40px;
}

.discount-select .coupon>view {
  float: left;
  padding: 5px;
}

.discount-select .coupon text {
  display: block;
  font-size: 14px;
}

.discount-select .coupon .small-text {
  font-size: 11px;
}

.discount-select image {
  width: 18px;
  min-height: 18px;
  border: solid 1px #eee;
  margin-top: 15px;
  margin-right: 5px;
  border-radius: 50%;
  float: right;
}

.discount-select button {
  border-radius: 0;
}

.express{
  padding:10px 15px;
  background: #ffffff;
  border-top: solid 1px #eeeeee;
  border-bottom: solid 1px #eeeeee;
}

.express image{
  width: 40px;
  position: relative;
  bottom: 8px;
  margin-right: 15px;
  vertical-align: middle;
}